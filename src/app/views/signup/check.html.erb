<% provide(:title, 'サインアップ') %>
<% provide(:main_header_text, 'サインアップ') %>
<% content_for :main_content do %>
  <%= form_with url: signup_entry_path, data: { turbo: false } do |f| %>
    <%= f.text_field :code, class: '' %>
    <%= f.label :code, '招待コード', class: '' %>
    <%= f.submit '次へ' %>
  <% end %>
<% end %>
<%= render 'layouts/default' %>