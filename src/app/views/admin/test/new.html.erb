<%= form_with url: admin_test_verify_path do |f| %>
  <%= f.label :public_key, style: "display: block" %>
  <%= f.text_area :public_key %>
  <br/>
  <%= f.label :signature, style: "display: block" %>
  <%= f.text_area :signature %>
  <br/>
  <%= f.label :message, style: "display: block" %>
